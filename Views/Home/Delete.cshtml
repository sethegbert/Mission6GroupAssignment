@{
    ViewData["Title"] = "Delete Movie";
}
@model Quadrant

<h2>Confirm Deletion</h2>
<h3>Are you sure you want to delete the task for: @Model.Task?</h3>

<form method="post" asp-action="Delete">
    <button class="greenButton" asp-controller="Home" asp-action="Quadrants">Cancel</button><br /><br />
    <button class="redButton" type="submit">Delete</button>

    <input type="hidden" asp-for="EntryId" />

</form>